/dts-v1/;

#include "ipq5332-mi01.2.dts"


/ {
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	model = "Qualcomm Technologies, Inc. IPQ5332/AP-MI01.2-QCN9160-C1";
	compatible = "qcom,ipq5332-ap-mi01.2-qcn9160-c1", "qcom,ipq5332";
	interrupt-parent = <&intc>;

	soc {
		pcie0_phy: phy@4b0000{
			status = "disabled";
		};

		pcie0: pcie@20000000 {
			status = "disabled";
		};

		ssuniphy_0: ssuniphy@4b0000 {
			status = "ok";
		};

		usb3: usb3@8A00000 {
			dwc_0: dwc3@8A00000 {
				#phy-cells = <0>;
				phys = <&ssuniphy_0>;
				phy-names = "usb3-phy";
			};
		};

		q6v5_wcss: remoteproc@d100000 {
			q6_wcss_pd2: remoteproc_pd2 {
				status = "ok";
			};
		};
	};
};
